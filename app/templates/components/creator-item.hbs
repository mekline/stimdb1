<div class="panel panel-default creator-item">

	<div class="panel-heading">
	  <h3 class="panel-title">{{item.name}}</h3>
	</div>

	<div class="panel-body">
		<p>Email Address: {{item.emailaddress}}</p>
		<p>Website: <a href={{item.website}}>{{item.website}}</a></p>
		<p>Source: {{item.creatorSource}}</p>
		<p>Stimulus Sets:</p>
		<ul>
		{{#each item.stimsets as |stimset|}}
               <li>{{stimset.stimsetname}}</li>
             {{/each}}
        </ul>
	</div>

	<div class="panel-footer text-right">
		{{yield}}
	</div>

</div>

