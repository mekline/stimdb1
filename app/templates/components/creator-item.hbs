<div class="panel panel-default creator-item">

	<div class="panel-heading">
	  <h3 class="panel-title">{{#link-to 'creators.detail' creator.id}}{{creator.name}}{{/link-to}}</h3>
	</div>

	<div class="panel-body">
		<p>Website: <a href={{creator.website}}>{{creator.website}}</a></p>
		<p>Stimulus Sets:</p>
		<ul>
		{{#each creator.stimsets as |stimset|}}
               <li>{{#link-to 'stimsets.detail' stimset.id}}{{stimset.stimsetname}}{{/link-to}}</li>
             {{/each}}
        </ul>
	</div>

	<div class="panel-footer text-right">
		{{yield}}
	</div>

</div>

