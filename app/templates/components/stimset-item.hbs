<div class="panel panel-default">

	<div class="panel-heading">
	  <h3 class="panel-title">{{#link-to 'stimsets.detail' stimset.id}}{{stimset.stimsetname}}{{/link-to}}</h3>
	</div>

	<div class="panel-body">
		<p>{{#link-to 'creators.detail' stimset.creator.id}}{{stimset.creator.name}}{{/link-to}}</p>
		<p>{{stimset.doi}}</p>
		<ul>
             {{#each stimset.stimuli as |stimulus|}}
               <li>{{#link-to 'stimuli.detail' stimulus.id}}{{stimulus.mediaFilename}}{{/link-to}}</li>
             {{/each}} 
         </ul>
	</div>
	<div class="panel-footer text-right">
	</div>

</div>